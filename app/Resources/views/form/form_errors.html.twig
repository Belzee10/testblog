
{% block form_errors -%}
    {% if errors|length > 0 -%}
        <ul class="error_list">
            {%- for error in errors -%}
                <li class="alert alert-danger">
                    <i class="fa fa-warning"></i>
                    {{ error.message }}
                </li>
            {%- endfor -%}
        </ul>
    {%- endif %}
{%- endblock form_errors %}